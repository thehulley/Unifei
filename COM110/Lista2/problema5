programa {
	funcao inicio() {
	    //Problema 5 - classe eleitoral//
	    
		inteiro idade
		
		escreva ("\nIdentificador de classe eleitoral\n\nDigite a sua idade:\n")
		leia (idade)
		
		se (idade < 16)
		{
		    escreva ("Você tem ", idade , " anos, portanto sua classe é a 'Não eleitor'\n")
		}
		
		se (idade >= 18 e idade <=65 )
		{
		    escreva ("Você tem ", idade , " anos, portanto sua classe é a 'Eleitor obrigatório'\n")
		}	
		
		se ((idade >= 16 e idade < 18) ou idade >= 66)
		{
		    escreva ("Você tem ", idade , " anos, portanto sua classe é a 'Eleitor facultativo'\n")
		}
		
		//fim codigo//
	}
}
